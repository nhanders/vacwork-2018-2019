#! c:/iverilog-x64/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1_1)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0000000002718b00 .scope module, "counter_tb" "counter_tb" 2 6;
 .timescale 0 0;
v000000000276bca0_0 .var "clk", 0 0;
v000000000276bd40_0 .net "count", 3 0, v000000000019d240_0;  1 drivers
v000000000276bde0_0 .var "enable", 0 0;
v000000000276be80_0 .var "reset", 0 0;
S_000000000019d020 .scope module, "U0" "counter" 2 10, 3 7 0, S_0000000002718b00;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "reset"
    .port_info 2 /INPUT 1 "enable"
    .port_info 3 /OUTPUT 4 "count"
v000000000019d1a0_0 .net "clk", 0 0, v000000000276bca0_0;  1 drivers
v000000000019d240_0 .var "count", 3 0;
v000000000019d2e0_0 .net "enable", 0 0, v000000000276bde0_0;  1 drivers
v000000000019d380_0 .net "reset", 0 0, v000000000276be80_0;  1 drivers
E_00000000027144f0 .event posedge, v000000000019d1a0_0;
    .scope S_000000000019d020;
T_0 ;
    %wait E_00000000027144f0;
    %load/vec4 v000000000019d380_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v000000000019d240_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v000000000019d2e0_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v000000000019d240_0;
    %addi 1, 0, 4;
    %assign/vec4 v000000000019d240_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0000000002718b00;
T_1 ;
    %vpi_call 2 22 "$display", "\011\011time,\011clk,\011reset,\011enable,\011count" {0 0 0};
    %vpi_call 2 23 "$monitor", "%d,\011%b,\011%b,\011%b,\011%d", $time, v000000000276bca0_0, v000000000276be80_0, v000000000276bde0_0, v000000000276bd40_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000000000276bca0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000000000276be80_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000000000276bde0_0, 0, 1;
    %delay 5, 0;
    %load/vec4 v000000000276bca0_0;
    %nor/r;
    %store/vec4 v000000000276bca0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000000000276be80_0, 0, 1;
    %delay 5, 0;
    %load/vec4 v000000000276bca0_0;
    %nor/r;
    %store/vec4 v000000000276bca0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000000000276be80_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000000000276bde0_0, 0, 1;
    %pushi/vec4 10, 0, 32;
T_1.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_1.1, 5;
    %jmp/1 T_1.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 5, 0;
    %load/vec4 v000000000276bca0_0;
    %nor/r;
    %store/vec4 v000000000276bca0_0, 0, 1;
    %jmp T_1.0;
T_1.1 ;
    %pop/vec4 1;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
